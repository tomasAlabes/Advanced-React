{
  "name": "sick-fits-backend",
  "version": "1.0.0",
  "description": "the backend of sick fits",
  "scripts": {
    "start": "ts-node src/index.ts",
    "dev": "ts-node-dev --respawn --transpileOnly -- src/index.ts",
    "debug": "ts-node-dev --respawn --transpileOnly --inspect -- src/index.ts",
    "test": "jest",
    "deploy": "prisma deploy --env-file variables.env",
    "generate-ts": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@types/cookie-parser": "1.4.1",
    "babel-preset-env": "^1.7.0",
    "bcryptjs": "2.4.3",
    "cookie-parser": "^1.4.3",
    "dotenv": "6.0.0",
    "graphql": "^0.13.2",
    "graphql-cli": "3.0.11",
    "graphql-yoga": "1.17.4",
    "jsonwebtoken": "8.3.0",
    "nodemailer": "^4.6.8",
    "prisma": "1.31.1",
    "prisma-binding": "2.1.6",
    "stripe": "^6.12.1",
    "typescript": "3.4.1"
  },
  "license": "MIT",
  "babel": {
    "presets": [
      "env"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.1.0",
    "@graphql-codegen/typescript": "1.1.0",
    "@graphql-codegen/typescript-operations": "1.1.0",
    "@graphql-codegen/typescript-react-apollo": "1.1.0",
    "@graphql-codegen/typescript-resolvers": "1.1.0",
    "@types/bcryptjs": "2.4.2",
    "@types/dotenv": "6.1.1",
    "@types/express": "4.16.1",
    "@types/graphql": "14.0.7",
    "@types/jest": "24.0.11",
    "@types/jsonwebtoken": "8.3.2",
    "@types/node": "11.11.3",
    "@types/nodemailer": "4.6.7",
    "@types/stripe": "6.25.7",
    "jest": "24.5.0",
    "ts-jest": "24.0.0",
    "ts-node-dev": "1.0.0-pre.32"
  }
}
